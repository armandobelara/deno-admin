{
  "$schema": "https://deno.land/x/denon@2.4.6/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run app.ts",
      "desc": "run my app.ts file",
      "allow": [
        "net"
      ],
      "tsconfig": "tsconfig.json"
    }
  },
  "watcher": {
    "match": [
      "app.ts",
      "src/**/*.ts"
    ]
  }
}